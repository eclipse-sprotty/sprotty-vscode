{
    "compilerOptions": {
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "ES2017",
        "lib": [
            "ES2017"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noImplicitOverride": true,
        "skipLibCheck": true,
        "strict": true,
        "strictPropertyInitialization": false
    }
}